{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "description": "Server side (API) code for Horizon Builder project",
  "main": "server.js",
  "author": "Jay Knapp <jpknapp@gmail.com>",
  "contributors": ["Jay Knapp <jpknapp@gmail.com>"],
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run copy-knex && npm run watch",
    "build": "npm run build-ts",
    "serve": "nodemon dist/server.js",
    "watch":
      "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "test":
      "SECRET_KEY=secretkey TOKEN_SECRET=tokensecret NODE_ENV=test jest --forceExit --runInBand \"spec.ts\"",
    "local-test":
      "NODE_ENV=test node -r dotenv/config node_modules/.bin/jest --forceExit --runInBand",
    "copy-knex": "cp src/knexfile.js dist/knexfile.js && cp -aR src/config/ dist/config",
    "format": "prettier --write 'src/**/*.{js,jsx,ts}' 'scripts/*.{js}'",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug":
      "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "heroku-prebuild": "npm install --only=dev"
  },
  "dependencies": {
    "@types/express": "^4.0.37",
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.26",
    "@types/swagger-jsdoc": "0.0.1",
    "aws-sdk": "^2.141.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.17.1",
    "compression": "^1.7.3",
    "concurrently": "^3.5.0",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.5.3",
    "debug": "~2.6.3",
    "docxtemplater": "^3.9.5",
    "docxtemplater-image-module": "3.1.0",
    "dotenv": "^4.0.0",
    "express": "~4.15.2",
    "express-session": "^1.15.5",
    "formidable": "^1.2.1",
    "fs": "^0.0.1-security",
    "html-pdf": "^2.2.0",
    "image-size": "^0.6.3",
    "jest": "^21.0.1",
    "jest-cli": "^21.0.2",
    "jszip": "2",
    "jwt-simple": "^0.5.1",
    "knex": "^0.13.0",
    "moment": "^2.18.1",
    "morgan": "~1.8.1",
    "nodemon": "^1.12.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^7.3.0",
    "phantom": "^4.0.12",
    "prettier": "1.12.1",
    "puppeteer": "^1.0.0",
    "ramda": "^0.25.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "serve-favicon": "~2.4.2",
    "supertest": "^3.0.0",
    "swagger-jsdoc": "^1.9.7",
    "typescript": "^2.5.2",
    "uuid": "^3.2.1",
    "xlsx": "^0.14.1",
    "xlsx-styles": "^0.8.21",
    "xml2js": "^0.4.19",
    "xmlbuilder": "^10.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.0.37",
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.26",
    "@types/swagger-jsdoc": "0.0.1",
    "concurrently": "^3.5.0",
    "dotenv": "^4.0.0",
    "jest": "^21.0.1",
    "jest-cli": "^21.0.2",
    "nodemon": "^1.12.0",
    "prettier": "1.12.1",
    "supertest": "^3.0.0",
    "swagger-jsdoc": "^1.9.7",
    "typescript": "^2.5.2"
  },
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js", "json"],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/jestPreprocessor.js"
    },
    "testMatch": ["**/__tests__/*.(ts|tsx|js)"],
    "testPathIgnorePatterns": ["./dist/*"]
  }
}
